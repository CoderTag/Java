package Practice;

public class ReverseString {

	public static void main(String[] args) {
		String s = "aSelenium";
		String rev = "";
		int ar[] = {1,2,3,4};
		
		
		for(int l=s.length()-1;l>=0;l--) {
			System.out.print(s.charAt(l));
			rev = rev + s.charAt(l);
		}
		System.out.println(rev);
		System.out.println(s.codePointAt(0));
		
	}

}
