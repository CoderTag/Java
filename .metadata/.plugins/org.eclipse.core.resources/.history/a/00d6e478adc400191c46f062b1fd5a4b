package Practice;

public class ReverseInteger {

	public static void main(String[] args) {
		int num = 1234;
		int rev=0;
		
		// One way
		while (num!=0)
		{
			rev = 10*rev + (num%10);
			num = num/10;
		}			
		System.out.print(rev);
		
		
		//2nd way
		num=12345;
		StringBuffer sb = new StringBuffer(num);
		System.out.print(sb.reverse());
	}

}
