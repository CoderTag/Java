package Practice;

public class ReverseInteger {

	public static void main(String[] args) {
		int num = 1;
		int rev=0;
				
		if (num < 10)
		{
			rev = num;
		}
		else
		{
			while (num>10)
			{
				rev = 10*rev + (num%10);
				num = num/10;
			}
			if (num<10)
			{
				rev = 10*rev + num;
			}
		}
		
		System.out.print(rev);
		

	}

}
